class FrenchFry {
  enum FrenchFrySize{ SMALL, MEDIUM, LARGE, ULTIMATE}
  FrenchFrySize size;
}

public class FrenchFryTest {

  public static void main(String args[]) {
    FrenchFry fry = new FrenchFry();
    fry.size = FrenchFry.FrenchFrySize.ULTIMATE ;
    System.out.println("Size:" + fry.size);
  }
}
